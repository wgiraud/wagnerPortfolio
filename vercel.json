{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "installCommand": "cd wagnerGiraud && npm ci",
  "buildCommand": "cd wagnerGiraud && npm run build",
  "outputDirectory": "wagnerGiraud/dist/wagner-giraud",
  "git": {
    "deploymentEnabled": {
      "*": false,
      "main": true
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
